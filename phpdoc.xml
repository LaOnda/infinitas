<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
	<parser>
		<default-package-name>Core</default-package-name>
		<title>Infinitas Cms 0.9</title>
		<target>./tmp/cache/</target>
		<extensions>
			<extension>php</extension>
		</extensions>
	</parser>
	<transformer>
		<target>./webroot/api</target>
		<template name="responsive"/>
	</transformer>
	<files>
		<directory>./Core</directory>
		<ignore>CakePHP/*</ignore>
		<ignore>*/Test/*</ignore>
		<ignore>*/Schema/*</ignore>
		<ignore>*/releases/*</ignore>
		<ignore>tmp/*</ignore>
		<ignore>webroot/*</ignore>
		<ignore>Core/GeoLocation/Lib/GeoIP/*</ignore>
	</files>
</phpdocumentor>

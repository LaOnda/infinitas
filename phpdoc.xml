<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
	<parser>
		<default-package-name>Infinitas CMS</default-package-name>
		<target>./tmp/cache/</target>
		<extensions>
			<extension>php</extension>
		</extensions>
	</parser>
	<transformer>
		<target>./webroot/api</target>
		<template name="responsive"/>
	</transformer>
	<files>
		<directory>./</directory>
		<ignore>CakePHP/*</ignore>
		<ignore>*/Test/*</ignore>
		<ignore>tmp/*</ignore>
		<ignore>webroot/*</ignore>
	</files>
</phpdocumentor>
